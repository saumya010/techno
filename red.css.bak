/* Import the necessary TK Bootstrap WP CSS additions */
/* My Minimal WordPress Styling comes here */
/* This is a small security for too long strings titles */
body {
  word-wrap: break-word;
  font-family: Roboto;
}
.admin-bar .navbar-fixed-top{
    top: 32px;
}
.sticky{}
.gallery-caption{}
.bypostauthor{}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: Oswald;
}
.magista_feature_content {
  text-align: center;
}
.magista_features {
  padding: 50px 0;
}
.magista_page-title {
  padding-bottom: 50px;
  text-align: center;
}
.magista_team {
  padding: 50px 0;
}
.magista_team .row{
    padding: 0px 70px;
    margin: 0 auto;
}
.col-xs-4{
    width:32%;
}
.col-xs-3{
    width: 24%;
}
.magista_grid{
    border: 4px solid steelblue;
    text-align: center;
    padding:0px;
    margin:0px 5px; 
}
.magista_grid h4{
    padding-top: 30px;
    padding-bottom: 8px;
}
.magista_grid p{
    padding-bottom: 8px;
}
.magista_video_section{
  text-align: center;
  background-color: #3695eb;
  padding: 50px 0;
}
a {
  color: #000000;
}
a:hover {
  color: #3999ee;
  text-decoration: none;
}
.magista_testimonial {
  background: url('http://publicdomainarchive.com/wp-content/uploads/2014/03/public-domain-images-archive-high-quality-resolution-free-download-0051.jpg') no-repeat fixed;
  padding: 50px 0;
}
.magista_testimonial .flexslider{
    background: none;
}
.magista_test-content {
  color: black;
  padding: 10px;
  width: 70%;
  margin-left: 20%;
  background-color: white;
  border-radius: 22px;
  text-align: center;
}
.magista_testimonial .magista_page-title {
  color: #b3b3b3;
}
.magista_test-image img{
    height: 100px !important;
    width: 100px !important;
    border-radius: 50%;
    border: 5px solid white;
}
.magista_test-image {
  height: 100px;
  width: 18%;
  float: left;
  padding-left: 90px;
}
.magista_gallery {
  text-align: center;
}
.magista_content img {
  border-radius: 50%;
  height: 250px;
  width: 250px;
  margin-bottom: 50px;
  border: 5px solid #3695eb;
}
.magista_post-img img {
  height: 250px;
}
.magista_foot {
  background-color: #3695eb;
  text-align: center;
  padding: 20px;
}
/* Alignment */
.alignleft {
  display: inline;
  float: left;
  margin-right: 1.5em;
}
.alignright {
  display: inline;
  float: right;
  margin-left: 1.5em;
}
.aligncenter {
  clear: both;
  display: block;
  margin: 0 auto;
}
/* Images and wp-captions should always fit and be responsive */
img {
  display: inline-block;
  height: auto;
  max-width: 100%;
}
img[class*="wp-image-"] {
  margin-top: 10px;
  margin-bottom: 10px;
}
.wp-caption {
  margin-bottom: 10px;
  max-width: 100%;
  border: 1px solid #ccc;
  border-radius: 4px;
  padding: 0 10px;
}
.wp-caption.aligncenter {
  margin-bottom: 10px;
}
.wp-caption img[class*="wp-image-"] {
  display: block;
}
.wp-caption .wp-caption-text {
  text-align: center;
  margin-top: 10px;
}
/* WP Gallery */
.gallery {
  margin-bottom: 1.5em;
}
.gallery dd {
  margin: 0;
}
/* Make sure embeds and iframes fit their containers */
embed,
iframe,
object {
  max-width: 100%;
}
/* Text meant only for screen readers */
.screen-reader-text {
  clip: rect(1px, 1px, 1px, 1px);
  position: absolute !important;
}
.screen-reader-text:hover,
.screen-reader-text:active,
.screen-reader-text:focus {
  background-color: #f1f1f1;
  border-radius: 3px;
  box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.6);
  clip: auto !important;
  color: #21759b;
  display: block;
  font-size: 14px;
  font-weight: bold;
  height: auto;
  left: 5px;
  line-height: normal;
  padding: 15px 23px 14px;
  text-decoration: none;
  top: 5px;
  width: auto;
  z-index: 100000;
  /* Above WP toolbar */
}
/* Content */
.main-content-inner {
  padding-bottom: 1.5em;
}
.hentry {
  margin: 0 0 1.5em;
}
/* Clearing */
.clear {
  clear: both;
}
/* Here some needed widget styles
 * Most widgets are adapted in the file bootstrap-wp.js - and REALLY easy to modify! ;) */
.widget {
  margin-bottom: 2em;
}
.widget_nav_menu ul.sub-menu li,
.widget_pages ul.children li {
  padding-left: 15px;
}
/* Hiding the search widget's button in widgets. thats just too old-skool. :) but if you want it back, just delete the next line.
 * Note: you can change the whole appearance of the search_form() function in our searchform.php */
.widget_search .search-form input[type="submit"] {
  display: none;
}
/* Make sure select elements fit in widgets */
.widget select {
  max-width: 100%;
}
/* Nested comments? They are threaded and nested already, you just need to add your own styling to finalize!
 * For example with some margin, like the line below ;) Delete, modify, change classes in HTML if you desire another style */
.comment .children {
  margin-left: 1.5em;
}
.magista_home-slider.flexslider{
    height: 580px;
}
.flexslider{
    border: none;
    box-shadow:none;
}
.slides img{
    height:580px;
}
.navbar-default{
    height: 51px;
    background-color: steelblue;
    border-color:transparent;
}
.navbar{
    border-radius: 0px;
}
.navbar-default .navbar-nav > li > a:hover{
    color:black;
    background-color: white;
}
.navbar-default .navbar-nav > li > a{
    color: white;
}
.navbar-default .navbar-brand:hover{
    color: white;
}
.navbar-default .navbar-brand{
    color:white;
}
.site-navigation-inner{
    height: 51px;
}
.magista_blog{
    clear: both;
    margin-bottom: 30px;
}
.magista_blog-inner{
    border: 1px solid grey;
    margin-bottom: 20px;
    margin: 0px 5px;
    padding:0px;
}
.magista_blog-content .magista_page-title{
    padding:0px;
}
.magista_blog-btm{
    padding-bottom: 15px;
}
.magista_social-icons{
    list-style: none;
    visibility:hidden;
    width: 180px;
    float:left;
    margin-left: 30px;
    margin-top: 4px;
}
.magista_grid:hover .magista_social-icons{
    visibility: visible;
    -webkit-transition: 0.7s ease-in-left;
}
.magista_social-icons li{
    display: inline-block;
    padding-right: 20px;
}
.magista_icon-share{
    margin-left: 230px;
    color:white;
    background-color: steelblue;
    padding:7px;
}
.magista_font_icon i{
    font-size: 30px;
}
.magista_post-content{
    padding:10px;
    text-align: left;
}
.magista_post-img img{
    padding-bottom: 20px;
    width: 100%;
}
.magista_posts-title{
    padding-bottom: 25px;
    text-align: center;
}
.magista_blog-img img{
    width: 100%;
}
.magista_blog-img{
    padding-bottom: 15px;
}
.magista_blog-content .row{
    padding: 0px 70px;
}

.form-allowed-tags{
    display:none;
}
.alert{
    display: none;
}
aside .nav > li > a:hover{
    background: none;
    color:black;
    padding-left: 25px;
    transition: all .3s ease;
}
.comment-form-author{
    width: 32.5%;
    display: inline-block;
    margin-right: 5px;
}
.comment-form-email{
    width: 32.5%; 
    display: inline-block;    
    margin-right: 5px;
}
.comment-form-url{
    width: 32.5%;
    display: inline-block;
}
.flex-direction-nav a.flex-next:before{
    content: '\f101';
    color: dodgerblue;
}
.flex-direction-nav a:hover{
    background-color: white;
    border-radius: 50%;
}
.flex-direction-nav a.flex-prev:before{
    content: '\f100';
    color: dodgerblue;
}
.flex-prev{
    text-align: center !important;
}
.flex-next{
    text-align: center !important;
}
.magista_foot{
    color:white;
}
.magista_foot a{
    color:white;
}
.magista_foot a:hover{
    color: white;
}
.page-header{
    border-bottom: 4px solid #bbb;
}
.page-header .magista_page-title{
    padding-bottom: 30px;
}
.widget_recent_comments li:before{
    font-family: 'FontAwesome' !important;
    margin-right: 7px !important;
    float: left !important;
    content: '\f075' !important;
    font-size: 12px !important;
    padding: 3px 0 0 !important;    
    color: black !important;
}
.sidebar{
    padding-left: 30px;
}
.sidebar ul{
    padding-left: 10px;
}
.sidebar li:before{
    font-family: 'FontAwesome';
    margin-right: 7px;
    float: left;
    content: '\f101';
    color: blue;
    font-size: 12px;
    padding: 7px 0 0;
}
.widget li a{
    padding:5px 15px;
}
.widget{
    border-bottom: 2px solid dodgerblue;
    padding-bottom: 10px;
}
@media screen and (max-width:1348px) {
    .magista_social-icons, .magista_icon-share{
        display:none;
    }
    .magista_grid img{
        height: auto;
    }
    .magista_grid h4{
        font-size: 14px;
    }
    .navbar-default .navbar-nav > li{
        width: 150px;
        text-align: center;
    }
    #main-menu{
        float:right;
    }
    .magista_test-image img{
        margin: 0 auto;
        margin-bottom: 15px;
    }
    .magista_test-image{
        clear: both;
        float:none;
        width: 100%;
        margin-bottom: 15px !important;
        padding: 0px;
    }
    .magista_test-content p{
        text-align: center;
    }
    .magista_test-content{
        margin: 0px;
        clear: both;
        float:none;
        width: 100%;
    }
    .flex-direction-nav{
        display:none;
    }
    .magista_blog{
        margin-bottom: 10px;
    }    
    .col-xs-3{
        width: 48%;
        margin-bottom: 15px;
    }
    .col-xs-4{
        width: 30%;
    }
    .magista_test-content{
        width: 100%;
    }
    .magista_test-image{
        margin: 0 auto;
    }
}
@media screen and (max-width:752px) {
    .navbar-default .navbar-nav > li > a:hover{
        background-color:whitesmoke;
        color: black;
    }
    .navbar-default .navbar-nav > li > a{
        background-color: steelblue;
        border-bottom:  1px solid grey;
    }    
    .navbar-default .navbar-toggle .icon-bar{
        background-color: white;
    }
    .navbar-default .navbar-toggle:hover{
        background-color: #ccc;
    }
    .comment-form-author{
       width: 100%;
    }
    .comment-form-email{
        width: 100%;
    }
    .comment-form-url{
        width: 100%;
    }
    .col-xs-4{
        width: 100%;
        clear: both;
    }
}
@media screen and (max-width:652px) {
    .col-xs-3,.col-xs-4{
        width: 100%;
        clear: both;
        margin-bottom: 15px; 
    }
}